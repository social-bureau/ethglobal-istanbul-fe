{
  "name": "social-bureau-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force --host",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "build:trydev": "tsc && vite build --mode trydev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "firebase:deploy:trydev": "tsc && vite build --mode trydev && firebase deploy --only hosting:trydev",
    "firebase:deploy:prod": "firebase deploy --only hosting:trydev",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:check && npm run lint"
    }
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@lens-protocol/client": "^2.0.0-alpha.21",
    "@metamask/eth-sig-util": "^4.0.1",
    "@metamask/providers": "^14.0.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@web3inbox/core": "^0.0.22",
    "@web3inbox/widget-react": "^0.6.25",
    "@web3modal/wagmi": "^3.3.2",
    "axios": "^1.5.1",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "bytes": "^3.1.2",
    "classnames": "^2.3.2",
    "crypto-browserify": "^3.12.0",
    "eciesjs": "^0.3.15",
    "ethers": "^5.6.4",
    "events": "^3.3.0",
    "firebase": "^10.5.2",
    "flowbite": "^1.8.1",
    "flowbite-react": "^0.6.4",
    "formik": "^2.4.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-icons": "^4.11.0",
    "react-loading-skeleton": "^3.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "react-spring-lightbox": "^1.8.0",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.1.1",
    "util": "^0.12.5",
    "viem": "^1.18.0",
    "wagmi": "^1.4.5",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.4",
    "@types/lodash": "^4.14.200",
    "@types/react": "^18.2.15",
    "@types/react-copy-to-clipboard": "^5.0.6",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.28",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^4.3.8",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-node-polyfills": "^0.16.0"
  }
